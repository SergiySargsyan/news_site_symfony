{% extends 'AppBundle::default_layout.html.twig' %}

{% block body %}
    <h1>All news page</h1>

    {% for news in pagination %}
        <h5><a href="{{ path('news_show_one', {id: news.id}) }}">{{ news.title }}</a></h5>

    {% endfor %}


    {{ dump(pagination) }}
    {# display navigation #}
    <div class="navigation">
        {{ knp_pagination_render(pagination) }}
    </div>
{% endblock %}
